<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.twistedappdeveloper.opcclient">

    <uses-permission android:name="android.permission.INTERNET" />
   <!-- <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SessionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SelectSession_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ListSubscriptionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SubscriptionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MonitoredItemActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BrowseActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".ReadActivity" />
        <activity android:name=".MonitoringActivity"></activity>
    </application>

</manifest>